<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Movies and Comments</title>
    <link rel="stylesheet" href="/styles.css">
</head>

<body>
    <h1>Movies</h1>
    <div id="movies">
        <% movies.forEach(movie=> { %>
            <p>
                <%= movie.title %>
            </p>
            <% }) %>
    </div>

    <h2>Comments</h2>
    <div id="comments">
        <% comments.forEach(comment=> { %>
            <p>
                <%= comment.text %>
            </p>
            <% }) %>
    </div>

    <script src="/scripts.js"></script>
</body>

</html>